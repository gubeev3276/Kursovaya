#!/bin/bash
# 1-ый тестовый сценарий
printf "Использование программы без параметра\n"
./main 
# 2-ый тестовый сценарий
printf "Использование программы с параметром -h\n"
./main -h
# 3-ый тестовый сценарий
printf "Использование программы со всеми необходимыми параметрами\n"
./main -f base.txt -p 33333 -e error.txt
